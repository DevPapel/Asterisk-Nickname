for i in range(5):
    if i == 0:
        print("     *" + "   ******" + "   ******")
    elif i == 2:
        print("     *" + "   ******" + "   ******")
    elif i == 3:
        print("*    *" + "   *" + "        *")
    elif i == 4:
        print("*    *" + "   *" + "        *")
    else:
        print("     *" + "   *" + "        *")
else:
    print("******" + "   ******" + "   *")
